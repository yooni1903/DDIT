import pymysql
 
# MySQL Connection 연결
conn = pymysql.connect(host='localhost', user='root', password='python',
                       db='python', charset='utf8')
 
curs = conn.cursor()   # 자바에서 statement와 같은 역할이다
 
sql =  """
    SELECT * FROM stock
    WHERE s_name = %s
    """
curs.execute(sql, "삼성전자")


rows = curs.fetchall()
print(rows.s_price)    

conn.close()