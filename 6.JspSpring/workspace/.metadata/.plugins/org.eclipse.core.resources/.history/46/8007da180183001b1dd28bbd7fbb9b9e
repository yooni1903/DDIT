<html>
<body>
<form action = 'image.do'>
<select name="image" id ="image">
	%option%
</select>
<input type="submit" value="전송" />
<script type="text/javascript">
	var select = document.querySelector('#image');
	select.onchange = function(event){
		event.target.form.submit();
	}
</script>
</form>
</body>
</html>
