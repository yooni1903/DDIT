package kr.or.ddit.enumpkg;

public enum OperatorType {
	PLUS('+'), 
	MINUS('-'), 
	MULTIPLY('*'), 
	DIBIDE('/');
	
	private interface RealOperator{
		double operate(double left, double right);
	}

	private OperatorType(char sign) {
		this.sign = sign;
	}
	
	private char sign;
	
	public char getSign() {
		return sign;
	}
	
	public double operate(double left, double right, ) {
		
	}
}
